"rofi-pass; xmodmap ~/.Xmodmap"
	Mod2+Mod4+Shift + space

"rofimoji"
	Mod2+Mod4 + e

"alacritty"
	Mod2+Mod4 + Return

"/home/marcel/bin/keyboard toggle ca us"
	Mod2+Mod4 + k

"gscreenshot-cli -cs"
	Print
"gscreenshot-cli -so"
	shift+Print
"gscreenshot-cli -s -f /tmp/gscreenshot.png && gimp /tmp/gscreenshot.png"
	control+shift+Print

"xbindkeys_show"
	control +shift + q
